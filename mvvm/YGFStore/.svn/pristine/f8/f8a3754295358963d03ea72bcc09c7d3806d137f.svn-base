package com.neufmer.ygfstore.binding.textview;

import android.databinding.BindingAdapter;
import android.widget.TextView;

import com.wangxing.code.mvvm.binding.command.BindingCommand;

/**
 * Created by WangXing on 2019/6/12.
 */
public class ViewAdapter {

    @BindingAdapter(value = {"hasSelect"}, requireAll = false)
    public static void setSelect(TextView view, final Boolean hasSelect) {
        view.setSelected(hasSelect);
    }

}
